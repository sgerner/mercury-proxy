PORT=3000
LOG_LEVEL=info

# Shared secret for HMAC verification (Vercel <-> Relay)
# Generate a random string: openssl rand -hex 32
RELAY_SHARED_SECRET=change_me_to_a_long_random_string

# Master key for DB encryption (AES-256)
# Must be a 32-byte key, base64 encoded.
# Generate with: openssl rand -base64 32
RELAY_ENCRYPTION_KEY_B64=

# Mercury API Base URL (default is production)
MERCURY_API_BASE=https://api.mercury.com
